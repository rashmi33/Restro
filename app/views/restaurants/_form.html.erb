<%= form_for @restaurant, local: true do |form| %>
  <p>
    <%= form.label :name %><br>
    <%= form.text_field :name %>
  </p>
  <p>
    <%= form.label :address %><br>
    <%= form.text_field :address %>
  </p>
   <p>
    <%= form.label :city %><br>
    <%= form.text_field :city %>
  </p>
   <p>
    <%= form.label :phone_no %><br>
    <%= form.text_field :phone_no %>
  </p>
   <p>
    <%= form.label :rating %><br>
    <%= form.text_field :rating %>
  </p>
   <p>
    <%= form.label :is_veg %><br>
    <%= form.text_field :is_veg %>
  </p>
   <p>
    <%= form.label :has_bar %><br>
    <%= form.text_field :has_bar %>
  </p>
  <p>
    <%= form.submit %>
  </p>
<% end %>